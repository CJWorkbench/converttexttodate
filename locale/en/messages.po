msgid "_spec.name"
msgstr "Convert text to date"

msgid "_spec.description"
msgstr "Convert text columns to date."

msgid "_spec.parameters.help.name"
msgstr "Text values must look like YYYY-MM-DD."

msgid "_spec.parameters.error_means_null.name"
msgstr "Convert non-dates to null"

#: converttexttodate.py:34
msgid "error.invalidIso8601Syntax"
msgstr ""
"Invalid date value in column “{column}”. Got “{value}”; expected “YYYY-"
"MM-DD”. Please clean the text column or enable 'Convert non-dates to "
"null'"

#: converttexttodate.py:61
msgid "error.invalidMonth"
msgstr ""
"Invalid month in column “{column}”, value “{value}”. Month must be "
"between 1 and 12. Please clean the text column or enable 'Convert non-"
"dates to null'"

#: converttexttodate.py:74
msgid "error.invalidDay"
msgstr ""
"Invalid day in column “{column}”, value “{value}”. The day does not exist"
" in that month. Please clean the text column or enable 'Convert non-dates"
" to null'"

